<?xml version="1.0"?>
<sdf version="1.7">
  <!-- Note:
       This file defines the model for a box with two degrees of freedom
       (DOFs): a) one DOF for the horizontal motion along the y axis and
       b) one DOF for the vertical motion along the z axis.
       To this end, this model defines a small "ghost body" between
       the box and the world so that we can attach two prismatic joints for
       these two DOFs. The ghost body has negligible mass.
  -->
  <model name="simple_maze">
    <!-- Ghost body of negligible mass. -->
    <link name="sphere_ghost_body">
      <inertial>
        <mass>1.0e-6</mass>
        <inertia>
          <ixx>1.0e-6</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.0e-6</iyy>
          <iyz>0</iyz>
          <izz>1.0e-6</izz>
        </inertia>
      </inertial>
    </link>
    <!-- The x slider will have an actuator with a corresponding input port
         in our MultibodyPlant model so that we can apply an external force.
    -->
    <joint name="sphere_x_slider" type="prismatic">
      <parent>world</parent>
      <child>sphere_ghost_body</child>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
    </joint>
    <!-- The y slider allows the box to freely move up and down. Thus, the
         box would free fall if it wasn't for the contact with the ground. -->
    <joint name="sphere_y_slider" type="prismatic">
      <parent>sphere_ghost_body</parent>
      <child>sphere_actuated</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>
    <!-- The model for a solid box of uniform density. -->
    <link name="sphere_actuated">
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.004</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.004</iyy>
          <iyz>0</iyz>
          <izz>0.004</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <sphere>
            <radius>0.1</radius>
          </sphere>
        </geometry>
        <material>
          <diffuse>1.0 0 0 1.0</diffuse>
        </material>
      </visual>
      <collision name="collision">
        <geometry>
          <sphere>
            <radius>0.1</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <!-- MultibodyPlant's time stepping only uses the static
                   coefficient of friction, <mu>. -->
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <!-- The model for a solid box of uniform density. -->
    <link name="box_ghost_body_x">
      <inertial>
        <mass>1.0e-6</mass>
        <inertia>
          <ixx>1.0e-6</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.0e-6</iyy>
          <iyz>0</iyz>
          <izz>1.0e-6</izz>
        </inertia>
      </inertial>
    </link>

    <link name="box_ghost_body_y">
      <inertial>
        <mass>1.0e-6</mass>
        <inertia>
          <ixx>1.0e-6</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1.0e-6</iyy>
          <iyz>0</iyz>
          <izz>1.0e-6</izz>
        </inertia>
      </inertial>
    </link>

    <joint name="box_x_slider" type="prismatic">
      <parent>world</parent>
      <child>box_ghost_body_x</child>
      <axis>
        <xyz>1 0 0</xyz>
        <!-- Drake attaches an actuator to all joints with a non-zero effort
             limit. We do not want an actuator for this joint. -->
        <limit>
          <effort>0</effort>
        </limit>
      	<dynamics>
      	  <damping>100.0</damping>
      	</dynamics>
      </axis>
    </joint>

    <joint name="y_slider" type="prismatic">
      <parent>box_ghost_body_x</parent>
      <child>box_ghost_body_y</child>
      <axis>
        <xyz>0 1 0</xyz>
        <!-- Drake attaches an actuator to all joints with a non-zero effort
             limit. We do not want an actuator for this joint and therefore we
             set a zero effort limit. -->
        <limit>
          <effort>0</effort>
        </limit>
      	<dynamics>
      	  <damping>100.0</damping>
      	</dynamics>
      </axis>
    </joint>

    <joint name="rotation" type="revolute">
      <parent>box_ghost_body_y</parent>
      <child>box</child>
      <axis>
        <xyz>0 0 1</xyz>
        <!-- Drake attaches an actuator to all joints with a non-zero effort
             limit. We do not want an actuator for this joint and therefore we
             set a zero effort limit. -->
        <limit>
          <effort>0</effort>
        </limit>
      	<dynamics>
      	  <damping>100.0</damping>
      	</dynamics>
      </axis>
    </joint>

    <link name="box">
      <inertial>
        <mass>5.0</mass>
        <inertia>
          <ixx>0.166666666666667</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.166666666666667</iyy>
          <iyz>0</iyz>
          <izz>0.166666666666667</izz>
        </inertia>
      </inertial>

      <visual name="box_visual">
        <geometry>
          <box>
            <size>0.999 0.999 0.999</size>
          </box>
        </geometry>
        <material>
          <diffuse>0.0 0.0 0.8 0.8</diffuse>
        </material>
      </visual>

      <collision name="collision">
        <geometry>
          <!--
          <box>
            <size>0.999 0.999 0.999</size>
          </box>
          -->
          <sphere>
            <radius>0.5</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.5</mu>
              <mu2>0.5</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <collision name="minus_x_lower_left_corner">
        <pose> -0.5 -0.5 -0.5 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>

      <collision name="minus_x_lower_right_corner">
        <pose> -0.5 0.5 -0.5 0 0 0 </pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>

      <collision name="plus_x_lower_left_corner">
        <pose> 0.5 -0.5 -0.5 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>

      <collision name="plus_x_lower_right_corner">
        <pose> 0.5 0.5 -0.5 0 0 0 </pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>

      <collision name="minus_x_upper_left_corner">
        <pose> -0.5 -0.5 0.5 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>

      <collision name="minus_x_upper_right_corner">
        <pose> -0.5 0.5 0.5 0 0 0 </pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>

      <collision name="plus_x_upper_left_corner">
        <pose> 0.5 -0.5 0.5 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>

      <collision name="plus_x_upper_right_corner">
        <pose> 0.5 0.5 0.5 0 0 0 </pose>
        <geometry>
          <sphere>
            <radius>1e-7</radius>
          </sphere>
        </geometry>
      </collision>
    </link>

    <link name="wall">
    <visual name='visual'>
      <pose> 0 0.75 0 0 0 0 </pose>
      <geometry>
        <box>
         <size>2.0 0.2 0.3</size>
        </box>
      </geometry>
      <material>
        <diffuse>0.75 0.6 0.5 0.8</diffuse>
      </material>
    </visual>

    <collision name='collision'>
      <pose> 0 0.75 0 0 0 0 </pose>
      <geometry>
        <box>
         <size>2.0 0.2 0.3</size>
        </box>
      </geometry>

      <surface>
        <friction>
        <ode>
          <mu>0.5</mu>
          <mu2>0.5</mu2>
        </ode>
        </friction>
      </surface>
    </collision>
    </link>
    <joint name="world_to_wall" type="fixed">
      <parent>world</parent>
      <child>wall</child>
      <axis>
        <xyz>1 0 0</xyz>
      </axis>
    </joint>
  </model>
</sdf>
